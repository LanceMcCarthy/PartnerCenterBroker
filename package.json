{
  "name": "partner-center-broker",
  "version": "1.2.0",
  "description": "A helper library that lets you manage your Microsoft Partner Center application submissions.",
  "main": "./lib/devCenter.js",
  "types": "./lib/devCenter.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc",
    "rebuild": "rimraf lib && tsc",
    "test": "jest",
    "generate-docs": "rimraf ./docs && typedoc src/ --out docs --excludePrivate",
    "publish-docs": "gh-pages -d docs -t",
    "format-write": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "prep": "npm run rebuild && npm run format-write && npm run test && npm run generate-docs",
    "package": "npm run prep && npm pack",
    "publish": "npm run package && npm publish && npm run publish-docs"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/LanceMcCarthy/PartnerCenterBroker.git"
  },
  "keywords": [
    "StoreBroker",
    "Microsoft",
    "Partner",
    "Center",
    "PartnerCenter",
    "Microsoft",
    "Store",
    "API",
    "Microsoft",
    "Store",
    "DevCenter",
    "DevCenter",
    "API"
  ],
  "author": "Lance McCarthy",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/LanceMcCarthy/PartnerCenterBroker/issues"
  },
  "homepage": "https://github.com/LanceMcCarthy/PartnerCenterBroker#readme",
  "dependencies": {
    "got": "^11.5.2",
    "jest-junit": "^11.1.0"
  },
  "devDependencies": {
    "@types/got": "^9.6.11",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "gh-pages": "^6.0.0",
    "jest": "^29.5.0",
    "jest-circus": "^29.5.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.24.0",
    "typescript": "^5.0.0"
  }
}
